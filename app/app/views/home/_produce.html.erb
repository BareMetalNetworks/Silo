   
         <!-- Portfolio Section-->
                <header class="sectionhead bg-primary text-white text-center" id="produce">
            <div class="container d-flex align-items-center flex-column">
                <!-- Masthead Avatar Image-->
                <!-- <img class="masthead-avatar mb-5" src="assets/img/avataaars.svg" alt="" /> -->
                <!-- Masthead Heading-->
               <h1 class="sectionhead-heading text-uppercase mb-0">Available Produce</h1> 
                <!-- Icon Divider-->
                <div class="divider-custom divider-light">
                    <div class="divider-custom-line"></div>
                    <div class="divider-custom-icon"><i class="<%= @dividerIcon %>"></i></div>
                    <div class="divider-custom-line"></div>
                </div>
                <!-- Masthead Subheading-->
                <h3 class="masthead-subheading font-weight-light mb-0">A current and up-to-date list of our ever changing assortment of fresh, locally grown, produce and herbs</h3>
            </div>
        </header>



        <section class="page-section portfolio" id="portfolio">
            <div class="container">
                <!-- Portfolio Section Heading-->
        
                </div>

                <!-- Portfolio Grid Items-->
                <div class="row justify-content-center">
                    


                    <% @produces.each do |produce| %>
                    <div class="col-md-6 col-lg-4 mb-5">
                    
                        <div class="portfolio-item mx-auto" data-toggle="modal" data-target="#portfolioModal1">
                            <div class="portfolio-item-caption d-flex align-items-center justify-content-center h-100 w-100">
                                <div class="portfolio-item-caption-content text-center text-white"><h4 class="my-3"><%= produce.name %></h4></div>
                            </div>
                            <%= image_tag produce.thumb, class: "img-fluid" %>
                            
                        </div>
                    </div>

                    <% end %>
        

 



                </div>
            </div>
        </section>